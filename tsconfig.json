{
    "compilerOptions": {
        "module": "commonjs",
        "declaration": true,
        "strict": true,
        "removeComments": false,
        "preserveConstEnums": true,
        "sourceMap": false,
        "target": "es2015",
        "noUnusedParameters": true,
        "noUnusedLocals": true,
        "moduleResolution": "node",
        "types": ["node"],
        "esModuleInterop": true
    },
    "include": [
        "index.ts"
    ]
}
